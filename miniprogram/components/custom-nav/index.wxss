.custom-nav-wrap {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;
  width: 100%;
  overflow-x: hidden;
}
.status-bar {
  height: 32px;
  background-color: #00d0a6;
}
.nav-bar {
  display: flex;
  justify-content: flex-start;
  height: 100%;
}
.nav-button {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.nav-search {
  height: 100%;
  background-color: #fff;
}
.back-icon {
  width: 80%;
  height: 80%;
}

.inv-nav-wrap {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
}