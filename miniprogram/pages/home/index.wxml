<view class="music {{ isPlay ? '' : 'music-pause' }}" bind:tap="audioPlay">
  <image class="music-icon" src="../../imgs/icon/music_icon.png" />
  <image class="music-play" src="../../imgs/icon/music_play.png" />
</view>

<view wx:if="{{isShowMessage}}" class="message-list-wrap" style="bottom: {{safeHeight + 64}}rpx;">
  <view class="message-list {{currentOffset ? 'message-list-animate' : ''}}" style="top: -{{currentOffset}}rpx;">
    <view wx:for="{{mList}}" wx:key="_id" class="message-list-item">{{item.bensonNickName}}：{{item.bensonWord}}</view>
  </view>
</view>

<view class="bottom-tool-bar" style="padding-bottom: {{safeHeight}}rpx;">
  <view class="message-ipt" bind:tap="openMessageModal">
    <text>留下您的祝福...</text>
    <image src="data:image/png;base64," />
    <button wx:if="{{!isAuth}}" class="message-ipt-btn" bindtap="getUserProfile"></button>
  </view>
  <view class="tool-zan" bind:tap="insertZanNumber">
    <image src="data:image/png;base64," />
    <view class="zan-number">
      <text class="zan-number-txt">{{zanNumber}}</text>
    </view>
  </view>
  <view class="tool-albums" bind:tap="goToAlbums">相册</view>
  <view class="tool-clear" bind:tap="onClearMessage">{{ isShowMessage ? '清屏' : '弹幕' }}</view>
</view>

<view wx:if="{{isModalShow}}" class="message-modal" catch:tap="onCloseModal">
  <view class="modal-content" catch:tap="catchEvent">
    <view class="modal-title">
      <view class="modal-title-padding"></view>
      <view>送祝福</view>
      <image bindtap="onCloseModal" src="data:image/png;base64," />
    </view>
    <view class="modal-input">
      <input value="{{bensonWord}}" bindinput="onBensonInput" auto-focus placeholder="输入祝福"/>
    </view>
    <view class="modal-submit-btn" bind:tap="insertMessageList">发送</view>
  </view>
</view>