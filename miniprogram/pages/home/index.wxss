/* 音乐播放按钮 */
.music {
  position: fixed;
  top: 10rpx;
  right: 24rpx;
  width: 80rpx;
  height: 80rpx;
  pointer-events: auto;
}
.music-pause .music-icon {
  animation: none;
}
.music-pause .music-play {
  transform: none;
}
.music-icon {
  margin-top: 15rpx;
  width: 60rpx;
  height: 60rpx;
  animation: musicRotate 2s linear infinite;
}
.music-play {
  width: 28rpx;
  height: 60rpx;
  margin-left: -14rpx;
  transform-origin: top;
  transform: rotate(20deg);
}

/* 弹幕样式 */
.message-list-wrap {
  position: fixed;
  z-index: 99;
  left: 12rpx;
  width: 400rpx;
  height: 200rpx;
  overflow: hidden;
  pointer-events: none;
}
.message-list {
  position: relative;
  width: 100%;
  height: auto;
}
.message-list-animate {
  transition: top 0.6s;
}
.message-list-item {
  width: 100%;
  height: 56rpx;
  border-radius: 56rpx;
  background-color: rgba(130, 151, 136, 0.5);
  margin-bottom: 10rpx;
  box-sizing: border-box;
  padding: 0 20rpx;
  color: #fff;
  font-size: 22rpx;
  align-items: center;
  overflow: hidden;
  text-overflow: ellipsis;
  display: flex;
  justify-content: center;
  align-items: center;
}
